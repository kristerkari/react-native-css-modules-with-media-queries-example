{
  "name": "TestMediaQueries",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . && stylelint '**/*.@(css|scss)'",
    "postinstall": "patch-package",
    "start": "react-native start",
    "test": "jest",
    "web": "webpack serve --mode development"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "es6-shim": "^0.35.5",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.6.1",
    "react-native": "0.72.1",
    "react-native-fontawesome": "^7.0.0",
    "react-native-orientation-change-provider": "^0.11.4",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.19.6",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/runtime": "^7.22.6",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-native-classname-to-dynamic-style": "^0.22.0",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react": "7.0.0-beta.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.19.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "mini-css-extract-plugin": "^2.7.6",
    "patch-package": "^7.0.0",
    "postcss": "^8.4.25",
    "postcss-css-variables": "^0.19.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.4.1",
    "react-hot-loader": "3.1.3",
    "react-native-css-transformer": "^1.2.4",
    "react-test-renderer": "18.2.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^0.23.1",
    "stylelint": "^15.10.1",
    "stylelint-config-react-native-css-modules": "^3.1.0",
    "stylelint-react-native": "^2.6.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "css-modules"
    ],
    "extends": [
      "plugin:css-modules/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
